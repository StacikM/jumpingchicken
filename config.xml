<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ctkgames.jumpingchicken" version="1.0.23" xmlns="http://www.w3.org/ns/widgets"
    xmlns:cdv="http://cordova.apache.org/ns/1.0"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <name>JumpingChickeniOS2</name>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <plugin name="cordova-plugin-screen-orientation" version="3.0.2" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />

    <!-- Allow iframes on iOS like leaderboards, including those in development that are not served
    via https. -->
    <allow-navigation href="*" />

    <platform name="android">
        <allow-intent href="market:*" />

        <!-- Required by admob-plus plugin > version 2 -->
        <preference name="GradlePluginKotlinEnabled" value="true" />

        <!-- Increase timeout value for low-end android devices -->
        <preference name="loadUrlTimeoutValue" value="60000" />

        <icon src="www/ad1d13c196d8ecdd00a8fa7a4a921fde731410ef9d58e2eab5be571c8c791e9c-jumpingchickenlogo.png" density="ldpi" />
<icon src="www/ad1d13c196d8ecdd00a8fa7a4a921fde731410ef9d58e2eab5be571c8c791e9c-jumpingchickenlogo.png" density="mdpi" />
<icon src="www/ad1d13c196d8ecdd00a8fa7a4a921fde731410ef9d58e2eab5be571c8c791e9c-jumpingchickenlogo.png" density="hdpi" />
<icon src="www/ad1d13c196d8ecdd00a8fa7a4a921fde731410ef9d58e2eab5be571c8c791e9c-jumpingchickenlogo.png" density="xhdpi" />
<icon src="www/ad1d13c196d8ecdd00a8fa7a4a921fde731410ef9d58e2eab5be571c8c791e9c-jumpingchickenlogo.png" density="xxhdpi" />
<icon src="www/ad1d13c196d8ecdd00a8fa7a4a921fde731410ef9d58e2eab5be571c8c791e9c-jumpingchickenlogo.png" density="xxxhdpi" />
<preference name="AndroidWindowSplashScreenAnimatedIcon" value="www/ad1d13c196d8ecdd00a8fa7a4a921fde731410ef9d58e2eab5be571c8c791e9c-jumpingchickenlogo.png" />

        <preference name="AndroidWindowSplashScreenBackground" value="#000000" />

        <!-- Required to get cordova-plugin-safariviewcontroller to call Chrome CustomTabs on
        Android. -->
        <config-file target="AndroidManifest.xml" parent="/manifest">
            <queries>
                <intent>
                    <action android:name="android.support.customtabs.action.CustomTabsService" />
                </intent>
            </queries>
        </config-file>
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />

        <preference name="SwiftVersion" value="5.3" />

        <!-- Required by admob-plus plugin > version 2 -->
        <preference name="deployment-target" value="12.0" />

